{
    "steps": [
        "签约信息",
        "经营信息"
    ],
    "items": [
        [
            {
                "name": "uid",
                "type": "input",
                "label": "绑定本站用户ID",
                "placeholder": "可留空"
            },
            {
                "name": "order_id",
                "type": "hidden",
                "label": "申请单编号"
            },
            {
                "name": "product",
                "type": "radio",
                "label": "选择开通的产品",
                "options": [],
                "value": "",
                "tip": "由于支付宝接口限制，一次性只能开通1个产品，可在开通成功后继续开通其他产品"
            },
            {
                "name": "rate",
                "type": "input",
                "label": "服务费率(%)",
                "validator": "numeric",
                "min": "0.38",
                "max": "0.6",
                "placeholder": "仅能填入2位以内小数，且在0.38~0.6范围内"
            },
            {
                "name": "alipay_account",
                "type": "input",
                "label": "签约支付宝账号",
                "placeholder": "填写你的支付宝账号",
                "note": "企业类型商户只能填写企业支付宝，个体工商户可填写个人或企业支付宝",
                "required": true
            },
            {
                "type": "line",
                "content": "营业执照"
            },
            {
                "name": "merchant_type",
                "type": "radio",
                "label": "商户类型",
                "value": "2",
                "options": [
                    {
                        "label": "个体工商户",
                        "value": "2"
                    },
                    {
                        "label": "企业",
                        "value": "3"
                    }
                ],
                "required": true
            },
            {
                "name": "business_license_pic",
                "type": "image-upload",
                "label": "营业执照照片",
                "filetype": "business",
                "autofill": {
                    "business_license_no": "license_no",
                    "merchant_name": "name"
                },
                "required": true
            },
            {
                "name": "business_license_no",
                "type": "input",
                "label": "营业执照编号",
                "placeholder": "上传营业执照后自动填入",
                "required": true
            },
            {
                "name": "merchant_name",
                "type": "input",
                "label": "营业执照名称",
                "placeholder": "上传营业执照后自动填入",
                "required": true
            },
            {
                "type": "line",
                "content": "联系人信息"
            },
            {
                "name": "contact_name",
                "type": "input",
                "label": "联系人姓名",
                "placeholder": "",
                "required": true
            },
            {
                "name": "contact_mobile",
                "type": "input",
                "label": "联系人手机号",
                "validator": "phone",
                "placeholder": "",
                "required": true
            },
            {
                "name": "contact_email",
                "type": "input",
                "label": "联系人邮箱",
                "validator": "email",
                "placeholder": ""
