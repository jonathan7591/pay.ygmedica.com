        ],
        [
            {
                "type": "line",
                "content": "经营类目"
            },
            {
                "name": "mcc",
                "type": "cascader",
                "label": "经营类目",
                "options": "${mcc}",
                "autofill": {
                    "need_special": "need_special"
                },
                "allnode": true,
                "required": true
            },
            {
                "name": "need_special",
                "type": "hidden",
